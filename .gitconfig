[remote "pikvm"]
    url = git@github.com:pikvm/pikvm.git
    fetch = +refs/heads/*:refs/remotes/pikvm/*
[remote "origin"]
    url = git@github.com:huangyingw/pikvm_pikvm.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = pikvm
    target = pikvm/master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/raspberry/pikvm/pikvm
